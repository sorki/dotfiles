#!/bin/bash
#
#
# Dotfiles info
# ==============
#
# Sensitive info removed
# -----------------------
#
# .gitconfig_ [ github token ]
#
#
# Now copy files without sensitive info

echo "Filename not printed twice == not overwritten"
for i in .*_; do
  echo $i;
  echo $i | sed s/_$// | xargs cp -v -n $i;
done
