Host *
  Compression yes
  CompressionLevel 7
  ControlMaster auto
  ControlPath /tmp/ssh-%r@%h:%p
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no

Host github.com
  User git
  Port 22
  Hostname github.com
  IdentityFile ~/.ssh/git_dsa
  TCPKeepAlive yes
  IdentitiesOnly yes

Host *.asu.cas.cz
  User sorki

Host v3.sk 77.93.223.108
 User sorki

Host aisa.fi.muni.cz
